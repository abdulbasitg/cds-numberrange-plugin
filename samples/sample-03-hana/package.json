{
  "name": "sample-03-hana",
  "version": "1.0.0",
  "description": "cds-numberrange-plugin sample project with HANA database.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2.1.1",
    "@cap-js/cds-test": "^0.3.0",
    "@sap/cds": "^9.0.3",
    "cds-numberrange-plugin": "file:cds-numberrange-plugin-1.0.0.tgz",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.11.0",
    "@sap/eslint-plugin-cds": "^4.0.2",
    "eslint": "^9.29.0"
  },
  "scripts": {
    "start": "cds-serve",
    "test": "cds bind --profile hybrid --exec cds test",
    "lint": "cds lint",
    "cds:watch": "cds watch",
    "cds:deploy": "cds deploy",
    "plugin:install": "npm pack ../../. && npm install cds-numberrange-plugin-*.tgz && rm cds-numberrange-plugin-*.tgz"
  },
  "cds": {
    "requires": {
      "[hybrid]": {
        "db": "hana"
      }
    },
    "cds-numberrange-plugin": {
      "ranges": [
        {
          "name": "BOOKID",
          "start": 1,
          "increment": 1
        },
        {
          "name": "BOOKIDPAD",
          "start": 1,
          "increment": 1,
          "additionalProperties": {
            "padCount": 10,
            "padValue": "0"
          }
        },
        {
          "name": "BOOKIDCHAR",
          "start": 1,
          "increment": 1,
          "additionalProperties": {
            "padCount": 8,
            "padValue": "0",
            "prefix": "A",
            "suffix": "Z"
          }
        }
      ]
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
