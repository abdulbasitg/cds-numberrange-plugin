{
  "name": "sample-04-draft",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^9.0.3",
    "cds-numberrange-plugin": "file:cds-numberrange-plugin-1.0.0.tgz",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.11.0",
    "@cap-js/sqlite": "^2.0.1"
  },
  "scripts": {
    "start": "cds-serve",
    "cds:watch": "cds watch",
    "cds:deploy": "cds deploy",
    "plugin:install": "npm pack ../../. && npm install cds-numberrange-plugin-*.tgz && rm cds-numberrange-plugin-*.tgz"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db.sqlite"
        }
      }
    },
    "cds-numberrange-plugin": {
      "ranges": [
        {
          "name": "BOOKID",
          "start": 100,
          "increment": 1,
          "createOnDraft": true
        },
        {
          "name": "AUTHORID",
          "start": 100,
          "increment": 1,
          "createOnDraft": false
        }
      ]
    }
  }
}
